{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon App.js",
    "heroku-postbuild": "npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@imranbarbhuiya/mongoose-fuzzy-searching": "^3.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fuzzy-search": "^3.2.1",
    "locus": "^2.0.4",
    "mongoose": "^6.4.4",
    "mongoose-fuzzy-search": "^0.0.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19"
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
